    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "cd server/src/config/client && npm build",
    "dev": "nodemon -x ts-node server/src/config/server.ts",
