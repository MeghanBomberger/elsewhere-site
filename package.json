    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "airtable": "^0.11.3",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "react-router-dom": "^6.2.2",
    "sass": "^1.49.9",
